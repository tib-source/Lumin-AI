{
    "version": 2,
    "builds": [
        {
            "src": "./server/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "./client",
            "use": "@vercel/vite"
        }
    ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/backend/server.js"
        }
      ]
}